<% include ../blocks/facultyheader.ejs %>
<div class="tm-right-column">
    <div class="tm-content-div">
       <section id="welcome" class="tm-section">

        
<h1>Student Question List</h1>
<hr>
<table class="table">
    <tr>
        <th>Question</th>
        <th>Date</th>
        <th>Student Name</th>
        <th>
            Send Answer
        </th>
    </tr>

    <% qus.forEach(rec=>
    { %>
        <tr>
            <th><%=rec.qus%></th>
            <th><%=rec.qdate%></th>
            <th><%=rec.name%></th>
            <th>
                <form>
                    <textarea></textarea>
                    <br>
                    <button>Send</button>
                </form>
            </th>
        </tr>
    <% })%>
</table>

</section></div></div>
<% include ../blocks/footer.ejs %>
