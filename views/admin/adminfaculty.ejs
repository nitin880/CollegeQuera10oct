<% include ../blocks/adminheader.ejs %>
                    
<div class="tm-right-column">
<div class="tm-content-div">
    <section id="welcome" class="tm-section">
        <h1>College Faculty List</h1>
        <hr>

        <table class="table">
            <tr>
                <th>Member ID</th>
                <th>Name</th>
                <th>Email</th>
            </tr>

            <% faculty.forEach(data=>
            { %>

                <tr>
                        <th><%=data.memberid%></th>
                        <th><%=data.name%></th>
                        <th><%=data.mail%></th>
                    </tr>

            <% }) %>

        </table>

    </section>
    
<% include ../blocks/footer.ejs %>